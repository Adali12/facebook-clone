
<h1 style="text-align:center;">Log in</h1>
<center>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <div class="form-group">
    <%= f.email_field :email,class:"form-control",id:"formGroupExampleInput", placeholder:"Enter Your Email",style:"width:400px;" %>
  </div>
  <div class="form-group">
    <%= f.password_field :password,class:"form-control",id:"formGroupExampleInput2", placeholder:"Enter Your Password",style:"width:400px;" %><br>
    <%= f.submit "Log in",class:"btn btn-primary" %>
  </div>
<% end %>
</center>
  
 

  
 

  


